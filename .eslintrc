{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true
  },
  "rules": {
    "strict": 0,
    "no-param-reassign": "error",
    "react/prefer-stateless-function": "off",
    "react/forbid-prop-types": "off",
    "no-underscore-dangle": "off"
  },
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ]
}